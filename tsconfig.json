{
    "compilerOptions": {
        "target": "es6",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "outDir": "./dist",
    },
    "include": ["package.json", "src/**/*"],
    "exclude": ["node_modules", "dist"]
}